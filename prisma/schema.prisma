generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("POSTGRES_PRISMA_URL")
}

model Example {
    id        String   @id @default(cuid())
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model User {
    id        String   @id @default(cuid())
    username  String?  @unique
    email     String?  @unique
    password  String
    image     String?
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
